  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "build:wait": "npm run build && sleep 5",
    "publish:alpha": "npm run build && npm version prerelease --preid=alpha && npm publish --tag alpha",
    "publish:beta": "npm run build && npm version prerelease --preid=beta && npm publish --tag beta",
    "patch": "npm version patch",
    "major": "npm version major",
    "pack": "npm run build && npm pack",
    "publish:latest": "npm publish --tag latest",
    "test:unit": "jest",
    "type-generate": "tsc --project tsconfig-types.json",
    "build": "rimraf -rf dist && npm run tsc",
    "clean": "rimraf -rf dist",
    "dev": "NODE_ENV=development nodemon --watch src --watch views, --watch typing -e \"*\" --delay 2.5 --exec 'npm run tsc'"
  },
